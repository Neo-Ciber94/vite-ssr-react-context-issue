{
    "compilerOptions": {
        "jsx": "react-jsx",
        "outDir": "./dist",
        "esModuleInterop": true,
        "isolatedModules": true,
        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "sourceMap": true,
        "incremental": true,
        "strict": true
    },
    "include": [
        "src",
        "vite.config.ts"
    ],
    "exclude": [
        "**/node_modules"
    ]
}